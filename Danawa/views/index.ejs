	<script>
		$(document).ready(function () {
			let weatherIcon = {
				'01': 'fas fa-sun',
				'02': 'fas fa-cloud-sun',
				'03': 'fas fa-cloud',
				'04': 'fas fa-cloud-meatball',
				'09': 'fas fa-cloud-sun-rain',
				'10': 'fas fa-cloud-showers-heavy',
				'11': 'fas fa-poo-storm',
				'13': 'fas fa-snowflake',
				'50': 'fas fa-smog',
			};

			$.ajax({
				url: 'http://api.openweathermap.org/data/2.5/weather?q=Seoul&appid=5d59ad0c6255bf93732fa429ec94b658&units=metric',
				dataType: "json",
				type: "GET",
				success: function (data) {
					var $Icon = (data.weather[0].icon).substr(0, 2);
					var $Temp = Math.floor(data.main.temp) + '°C';
					var $city = data.name;

					$('.CurrIcon').append('<i class="' + weatherIcon[$Icon] + '"></i>');
					$('.City').append($city);
					$('.CurrTemp').prepend($Temp);

				}
			})
		});
	</script>
	<script src="/js/handwrite.js"></script>
	<script>
	window.onload = function handIndex() {
	const annotate = RoughNotation.annotate;
	const annotationGroup = RoughNotation.annotationGroup;
	const $ = (t) => document.querySelector(t);

	{
		const a1 = annotate($('#indexUnderline'), { type: 'underline', color: '#FFCCFF'});
		const a2 = annotate($('#indexBox'), { type: 'box', color: '#FFFF99'});
		const a3 = annotate($('#indexUnderline2'), { type: 'underline', color: '#9EEEFA'});
		const a4 = annotate($('#indexBox2'), { type: 'box', color: '#f88aaf'});
		const a5 = annotate($('#indexUnderline3'), { type: 'underline', color: '#FFFF99'});
		const a6 = annotate($('#indexUnderline4'), { type: 'underline', color: '#a1d8b1', padding: 2});
		const a7 = annotate($('#indexCircle'), { type: 'circle', color: '#f88aaf', padding: 4});
		const ag = annotationGroup([a1, a2, a3, a4, a5, a6, a7]);
		ag.show();
	}
	}
	</script>

