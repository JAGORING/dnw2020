				dataType: "json",
				type: "GET",
				success: function (data) {
					var $Icon = (data.weather[0].icon).substr(0, 2);
					var $Temp = Math.floor(data.main.temp) + 'Â°C';
					var $city = data.name;

					$('.CurrIcon').append('<i class="' + weatherIcon[$Icon] + '"></i>');
					$('.City').append($city);
					$('.CurrTemp').prepend($Temp);

				}
			})
		});
	</script>
